    <owl:Ontology rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone">
        <owl:versionIRI rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone/1.0.1"/>
