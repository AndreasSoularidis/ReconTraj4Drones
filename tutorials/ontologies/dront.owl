    <!-- http://i-lab.aegean.gr/kotis/ontologies/onto4drone#ConstantRecordingPosition -->

    <owl:Class rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#ConstantRecordingPosition">
        <rdfs:subClassOf rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#RecordingPosition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#hasOccurredEvent"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#RecordingEvent"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#produces"/>
                                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                                <owl:onClass rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Record"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The spatiotemporal position on which recording -constantly- occurs.</rdfs:comment>
        <rdfs:label xml:lang="en">Constant Recording Position</rdfs:label>
    </owl:Class>
    


