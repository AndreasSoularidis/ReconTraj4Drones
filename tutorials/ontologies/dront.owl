            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#Drone"/>
        <rdfs:comment xml:lang="en">Specifies the drone that participates in an event.</rdfs:comment>
        <rdfs:label xml:lang="en">has drone participant</rdfs:label>
    </owl:ObjectProperty>
    


