        <owl:inverseOf rdf:resource="http://i-lab.aegean.gr/kotis/ontologies/onto4drone#isDroneParticipantIn"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
